{
  "name": "gh-pages",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "touch": "touch out/.nojekyll",
    "addfiles": "git add .",
    "commit": "git commit -m \"Deploy to Github pages\"",
    "subtree": "git subtree push --prefix out origin master",
    "deploy": "yarn build && yarn export && yarn touch && yarn addfiles && yarn commit && yarn subtree"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.14",
    "@next/mdx": "^9.4.4",
    "next": "latest",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7",
    "typescript-eslint-parser": "^22.0.0"
  }
